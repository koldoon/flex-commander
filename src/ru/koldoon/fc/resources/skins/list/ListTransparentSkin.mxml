<?xml version="1.0" encoding="utf-8"?>
<s:Skin
    xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
    alpha.disabled="0.5" blendMode="normal">

    <fx:Metadata>
        [HostComponent("spark.components.List")]
    </fx:Metadata>

    <s:states>
        <s:State name="normal"/>
        <s:State name="disabled"/>
    </s:states>

    <s:Scroller
        left="0" top="0" right="0" bottom="0" id="scroller"
        minViewportInset="0" hasFocusableChildren="false"
        skinClass="ru.koldoon.fc.resources.skins.scroller.ScrollerLightSkin">

        <s:DataGroup
            id="dataGroup" itemRenderer="spark.skins.spark.DefaultItemRenderer">

            <s:layout>
                <s:VerticalLayout
                    gap="0" horizontalAlign="justify" requestedMinRowCount="5"/>
            </s:layout>
        </s:DataGroup>
    </s:Scroller>
</s:Skin>
